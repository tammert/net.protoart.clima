{
  "type": "mdns-sd",
  "mdns-sd": {
    "name": "http",
    "protocol": "tcp"
  },
  "id": "{{txt.id}}",
  "conditions": [
    [
      {
        "field": "txt.name",
        "match": {
          "type": "regex",
          "value": "^HVAC_"
        }
      },
      {
        "field": "txt.model",
        "match": {
          "type": "regex",
          "value": "^LG_"
        }
      }
    ]
  ]
}
